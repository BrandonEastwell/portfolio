---
import  SpeedInsights  from "@vercel/speed-insights/astro"
import Analytics from '@vercel/analytics/astro'
---
<!doctype html>
<html lang="en">
	<head>
		<meta charset="UTF-8" />
		<meta name="viewport" content="width=device-width" />
		<link rel="icon" type="image/png" href="/favicon-96x96.png" sizes="96x96" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<link rel="shortcut icon" href="/favicon.ico" />
		<meta name="generator" content={Astro.generator} />
		<title>Brandon Eastwell's Portfolio</title>
		<meta name="description" content="Full-Stack, Software Engineer Portfolio">
	</head>
	<SpeedInsights />
	<Analytics />
	<body class="min-h-screen h-auto w-screen overflow-x-hidden bg-gradient-to-bl from-[#212121] from-5% to-[#1A1A1B] to-50%">
		<slot />
	</body>
</html>