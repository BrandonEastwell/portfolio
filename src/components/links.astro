---
import linkedin from '../assets/linkedin.svg';
import github from '../assets/github.svg';
import download from '../assets/download.svg';
---

<div class="flex flex-row gap-4">
    <a class="h-6 w-6 z-50" href="https://www.linkedin.com/in/brandon-eastwell-40056631b/" target="_blank">
        <img class="h-6 opacity-80 transition duration-300 hover:opacity-100 cursor-pointer" src={linkedin.src}  alt="" />
    </a>
    <a class="h-6 w-6 z-50" href="https://github.com/BrandonEastwell/" target="_blank">
        <img class="h-6 opacity-80 transition duration-300 hover:opacity-100 cursor-pointer" src={github.src}  alt="" />
    </a>
    <div class="flex flex-row gap-1 border-1 border-white px-3 rounded-lg place-items-center cursor-pointer opacity-80 transition duration-300 hover:opacity-100">
        <img class="h-5" src={download.src}  alt="" />
        <p class="text-white font-jersey">Resume</p>
    </div>
</div>